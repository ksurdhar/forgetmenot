<h2>Sign In</h2>

<%= render "layouts/errors" %>

<form action="<%= session_url %>" method="post">
  <%= auth_token_input %>

  <label>
    Username
    <input type="text" name="user[username]" value="">
  </label>

  <label>
    Password
    <input type="password" name="user[password_digest]" value="">
  </label>

  <input type="submit" value="Sign In">
</form>

